{
  "name": "CordovaHMSPushDemo",
  "displayName": "CordovaHMSPushDemo",
  "version": "6.1.0-300",
  "description": "A demo application which shows the usage of HMS Push SDK",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "platadd": "cordova platform add android",
    "platrm": "cordova platform rm android",
    "plgadd": "cordova plugin add ../../C-PUSH",
    "plgrm": "cordova plugin rm cordova-plugin-hms-push",
    "z": "npm run plgrm && npm run plgadd && npm run custom",
    "x": "npm run plgrm && npm run platrm && npm run platadd && npm run plgadd",
    "browser": "cordova run browser",
    "android": "cordova run android --debug -- --buildConfig=build.json",
    "release": "cordova run android --release -- --buildConfig=build.json",
    "custom": "cordova run android --debug"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": []
  },
  "files": [
    "./.docs",
    "./www",
    "config.xml",
    "LICENCE",
    "package.json",
    "README.md"
  ]
}