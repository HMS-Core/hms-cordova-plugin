{
    "compileOnSave": true,
    "compilerOptions": {
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "inlineSources": true,
        "outDir": "www",
        "module": "commonjs",
        "target": "es2015",
        "declaration": true,
        "declarationDir": "types"
    },
    "exclude": ["node_modules", "**/*.spec.ts", "types", "ionic-native"]
}
